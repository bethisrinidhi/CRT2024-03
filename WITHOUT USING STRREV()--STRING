WITHOUT USING STRREV() FUNCTION--STRINGS

#include <stdio.h>
#include<string.h>

int main() {
    // Write C code here
    char str[50],str1[50];
    scanf("%s",str);
    int n,i,j=0;
    n=strlen(str);
    for(i=n-1;i>=0;i--){
        str1[j]=str[i];
        j++;
    }
    str1[j]='\0';
    printf("%s",str1);

    return 0;
}
