k3s5i9 EXAMPLE STRINGS:

#include <stdio.h>
#include<string.h>

int main() {
    // Write C code here
    char str[50];
    scanf("%[^\n]s",str); 
    
int n,i,j;
n=strlen(str);
for(i=1;i<n;i=i+2){
    int r=(int)str[i]-48;
    for(j=1;j<=r;j++){
        printf("%c",str[i-1]);
    }
}
    return 0;
}
